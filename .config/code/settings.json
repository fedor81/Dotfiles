{
  "editor.fontSize": 16,
  "settingsSync.ignoredExtensions": [],
  "security.workspace.trust.untrustedFiles": "open",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "git.openRepositoryInParentFolders": "always",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.editorAssociations": {
    "*.csproj": "default"
  },
  "tabnine.experimentalAutoImports": true,
  "editor.linkedEditing": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "explorer.confirmDelete": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.enableNeovim": true,
  "vim.neovimConfigPath": "~/.config/nvim/init.lua",
  "vim.neovimUseConfigFile": true,
  "terminal.integrated.fontSize": 14,
  "vim.sneak": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.handleKeys": {
    "<C-f>": false
  },
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    }
  ],
  // To improve performance
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1,
    "asvetliakov.vscode-neovim": 1
  },
  "editor.codeLens": false,
  "debug.console.fontSize": 14,
  "scm.inputFontSize": 14,
  "chat.editor.fontSize": 14,
  "editor.fontFamily": "'JetBrainsMono Nerd Font', Menlo, Monaco, 'Courier New', monospace",
  "arena.enableTabAutocomplete": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "vim.smartRelativeLine": true,
  "files.autoSaveWhenNoErrors": true,
  "editor.codeActions.triggerOnFocusChange": true,
  "files.autoSave": "afterDelay",
  "python.createEnvironment.trigger": "off",
  "explorer.confirmDragAndDrop": false,
  "workbench.iconTheme": "vscode-icons",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "files.trimFinalNewlines": true,
  "git.confirmSync": false,
  "mssql.connections": [
    {
      "server": "localhost",
      "database": "master",
      "authenticationType": "SqlLogin",
      "user": "sa",
      "password": "",
      "emptyPasswordInput": false,
      "savePassword": true,
      "encrypt": "Mandatory",
      "trustServerCertificate": true,
      "connectTimeout": 15,
      "commandTimeout": 30,
      "applicationName": "vscode-mssql",
      "displayName": "localhost, master (sa)"
    }
  ],
  "editor.rulers": [100],
  "omnisharp.organizeImportsOnFormat": true,
  "explorer.autoReveal": "focusNoScroll",
  "cSpell.languageSettings": [],
  "cSpell.ignoreWords": ["Полагаю"],
  "cSpell.language": "en,ru",
  "mssql.enableRichExperiences": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "jupyter.askForKernelRestart": false,
  "codeium.enableConfig": {
    "*": true,
    "asm": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[nasm]": {},
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "editor.cursorSmoothCaretAnimation": "on",
  "vscode_custom_css.imports": [
    "file:///Users/komputer/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
  ],
  "workbench.colorTheme": "Palenight Theme"
}
